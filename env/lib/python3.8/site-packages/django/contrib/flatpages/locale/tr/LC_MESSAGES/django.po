#: env/lib/python3.8/site-packages/django/contrib/flatpages/admin.py:12
msgid "Advanced options"
msgstr "Gelişmiş seçenekler"

#: env/lib/python3.8/site-packages/django/contrib/flatpages/apps.py:7
msgid "Flat Pages"
msgstr "Düz Sayfalar"

#: env/lib/python3.8/site-packages/django/contrib/flatpages/forms.py:9
#: env/lib/python3.8/site-packages/django/contrib/flatpages/models.py:9
msgid "URL"
msgstr "URL"

#: env/lib/python3.8/site-packages/django/contrib/flatpages/forms.py:12
msgid ""
"Example: “/about/contact/”. Make sure to have leading and trailing slashes."
msgstr ""
"Örnek: “/hakkinda/iletisim/”. Başında ve sonunda bölü işareti olduğundan "
"emin olun."

#: env/lib/python3.8/site-packages/django/contrib/flatpages/forms.py:15
msgid ""
"This value must contain only letters, numbers, dots, underscores, dashes, "
"slashes or tildes."
msgstr ""
"Bu değer sadece harfler, sayılar, noktalar, altçizgiler, tireler, bölme veya "
"yaklaşık işaretleri içermek zorundadır."

#: env/lib/python3.8/site-packages/django/contrib/flatpages/forms.py:29
msgid "Example: “/about/contact”. Make sure to have a leading slash."
msgstr ""
"Örnek: “/hakkinda/iletisim”. Başında bölü işareti olduğundan emin olun."

#: env/lib/python3.8/site-packages/django/contrib/flatpages/forms.py:42
msgid "URL is missing a leading slash."
msgstr "URL başında bölü işareti eksik."

#: env/lib/python3.8/site-packages/django/contrib/flatpages/forms.py:47
msgid "URL is missing a trailing slash."
msgstr "URL sonunda bölü işareti eksik."

#: env/lib/python3.8/site-packages/django/contrib/flatpages/forms.py:64
#, python-format
msgid "Flatpage with url %(url)s already exists for site %(site)s"
msgstr "%(url)s adresli düz sayfa %(site)s sitesinde zaten mevcut"

#: env/lib/python3.8/site-packages/django/contrib/flatpages/models.py:10
msgid "title"
msgstr "başlık"

#: env/lib/python3.8/site-packages/django/contrib/flatpages/models.py:11
msgid "content"
msgstr "içerik"

#: env/lib/python3.8/site-packages/django/contrib/flatpages/models.py:12
msgid "enable comments"
msgstr "yorumları etkinleştir"

#: env/lib/python3.8/site-packages/django/contrib/flatpages/models.py:14
msgid "template name"
msgstr "şablon adı"

#: env/lib/python3.8/site-packages/django/contrib/flatpages/models.py:18
msgid ""
"Example: “flatpages/contact_page.html”. If this isn’t provided, the system "
"will use “flatpages/default.html”."
msgstr ""
"Örnek: “flatpages/contact_page.html”. Eğer bu şekilde yazılmazsa, sistem "
"“flatpages/default.html” olarak kullanacak."

#: env/lib/python3.8/site-packages/django/contrib/flatpages/models.py:23
msgid "registration required"
msgstr "kayıt olma gerekli"

#: env/lib/python3.8/site-packages/django/contrib/flatpages/models.py:24
msgid "If this is checked, only logged-in users will be able to view the page."
msgstr ""
"Eğer bu işaretlenirse, sadece oturum açan kullanıcılar sayfayı "
"görüntüleyebilecektir."

#: env/lib/python3.8/site-packages/django/contrib/flatpages/models.py:27
msgid "sites"
msgstr "siteler"

#: env/lib/python3.8/site-packages/django/contrib/flatpages/models.py:31
msgid "flat page"
msgstr "düz sayfa"

#: env/lib/python3.8/site-packages/django/contrib/flatpages/models.py:32
msgid "flat pages"
msgstr "düz sayfalar"
