#: env/lib/python3.8/site-packages/django/contrib/postgres/apps.py:42
msgid "PostgreSQL extensions"
msgstr "PostgreSQL uzantıları"

#: env/lib/python3.8/site-packages/django/contrib/postgres/fields/array.py:21
#: env/lib/python3.8/site-packages/django/contrib/postgres/forms/array.py:16
#: env/lib/python3.8/site-packages/django/contrib/postgres/forms/array.py:170
#, python-format
msgid "Item %(nth)s in the array did not validate:"
msgstr "Dizilimdeki %(nth)s öğesi doğrulanmadı:"

#: env/lib/python3.8/site-packages/django/contrib/postgres/fields/array.py:22
msgid "Nested arrays must have the same length."
msgstr "İç içe dizilimler aynı uzunlukta olmak zorunda."

#: env/lib/python3.8/site-packages/django/contrib/postgres/fields/hstore.py:16
msgid "Map of strings to strings/nulls"
msgstr "Dizgiler/boşlar olarak dizgilerin eşlemesi"

#: env/lib/python3.8/site-packages/django/contrib/postgres/fields/hstore.py:18
#, python-format
msgid "The value of “%(key)s” is not a string or null."
msgstr "“%(key)s” değeri bir dizgi ya da boş değil."

#: env/lib/python3.8/site-packages/django/contrib/postgres/fields/jsonb.py:32
msgid "A JSON object"
msgstr "JSON nesnesi"

#: env/lib/python3.8/site-packages/django/contrib/postgres/fields/jsonb.py:34
msgid "Value must be valid JSON."
msgstr "Değer geçerli JSON olmak zorundadır."

#: env/lib/python3.8/site-packages/django/contrib/postgres/forms/hstore.py:16
msgid "Could not load JSON data."
msgstr "JSON verisi yüklenemedi."

#: env/lib/python3.8/site-packages/django/contrib/postgres/forms/hstore.py:17
msgid "Input must be a JSON dictionary."
msgstr "Bir JSON dizini girilmek zorundadır."

#: env/lib/python3.8/site-packages/django/contrib/postgres/forms/jsonb.py:19
#, python-format
msgid "“%(value)s” value must be valid JSON."
msgstr "“%(value)s” değeri geçerli JSON olmak zorundadır."

#: env/lib/python3.8/site-packages/django/contrib/postgres/forms/ranges.py:37
msgid "Enter two valid values."
msgstr "Iki geçerli değer girin."

#: env/lib/python3.8/site-packages/django/contrib/postgres/forms/ranges.py:38
msgid "The start of the range must not exceed the end of the range."
msgstr "Aralığın başlangıcı aralığın bitişini aşmamak zorundadır."

#: env/lib/python3.8/site-packages/django/contrib/postgres/forms/ranges.py:86
msgid "Enter two whole numbers."
msgstr "Bütün iki sayıyı girin."

#: env/lib/python3.8/site-packages/django/contrib/postgres/forms/ranges.py:92
msgid "Enter two numbers."
msgstr "İki sayı girin."

#: env/lib/python3.8/site-packages/django/contrib/postgres/forms/ranges.py:109
msgid "Enter two valid date/times."
msgstr "Geçerli iki tarih/saat girin."

#: env/lib/python3.8/site-packages/django/contrib/postgres/forms/ranges.py:115
msgid "Enter two valid dates."
msgstr "Geçerli iki tarih girin."

#: env/lib/python3.8/site-packages/django/contrib/postgres/validators.py:12
#, python-format
msgid ""
"List contains %(show_value)d item, it should contain no more than "
"%(limit_value)d."
msgid_plural ""
"List contains %(show_value)d items, it should contain no more than "
"%(limit_value)d."
msgstr[0] ""
"Liste %(show_value)d öğe içeriyor, %(limit_value)d değerden daha fazla "
"içermemelidir."
msgstr[1] ""
"Liste %(show_value)d öğe içeriyor, %(limit_value)d değerden daha fazla "
"içermemelidir."

#: env/lib/python3.8/site-packages/django/contrib/postgres/validators.py:19
#, python-format
msgid ""
"List contains %(show_value)d item, it should contain no fewer than "
"%(limit_value)d."
msgid_plural ""
"List contains %(show_value)d items, it should contain no fewer than "
"%(limit_value)d."
msgstr[0] ""
"Liste %(show_value)d öğe içeriyor, %(limit_value)d değerden daha az "
"içermemelidir."
msgstr[1] ""
"Liste %(show_value)d öğe içeriyor, %(limit_value)d değerden daha az "
"içermemelidir."

#: env/lib/python3.8/site-packages/django/contrib/postgres/validators.py:29
#, python-format
msgid "Some keys were missing: %(keys)s"
msgstr "Bazı anahtarlar eksik: %(keys)s"

#: env/lib/python3.8/site-packages/django/contrib/postgres/validators.py:30
#, python-format
msgid "Some unknown keys were provided: %(keys)s"
msgstr "Bazı bilinmeyen anahtarlar verilmiş: %(keys)s"

#: env/lib/python3.8/site-packages/django/contrib/postgres/validators.py:70
#, python-format
msgid ""
"Ensure that this range is completely less than or equal to %(limit_value)s."
msgstr ""
"Bu aralığın %(limit_value)s değerinden küçük veya eşit olduğundan emin olun."

#: env/lib/python3.8/site-packages/django/contrib/postgres/validators.py:76
#, python-format
msgid ""
"Ensure that this range is completely greater than or equal to "
"%(limit_value)s."
msgstr ""
"Bu aralığın %(limit_value)s değerinden büyük veya eşit olduğundan emin olun."
